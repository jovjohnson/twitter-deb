<!DOCTYPE html>
<html>
  <head>
    <title>Who Is Winning?</title>

    <!-- STYLES -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans+Condensed:300' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Amatic+SC:400,700' rel='stylesheet' type='text/css'>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <!-- JAVASCRIPTS -->
    <script src="https://code.jquery.com/jquery-3.1.0.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.7/js/materialize.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.0/angular.min.js"></script>
    <script src="/bower_components/chart.js/dist/Chart.min.js"></script>
    <script src="/javascripts/angular-chart.min.js"></script>
    <script src="/javascripts/module.js"></script>
    <script src="/javascripts/services.js"></script>
    <script src="/javascripts/controllers.js"></script>
  </head>
  <body ng-app='debateApp' ng-controller='mainController'>
    <div class="container">
      <div class="head">
        <h1 class="pg-title">Who Is Winning?</h1>
      </div>
      <br>
      <!-- <div class="row"> -->
        <div class="col-md-12 lead" ng-model="winner">
          <span ng-if="response">
            <img class="hillary" src="/images/Running-Hillary_Clinton.png" alt=""><h1 class="winner">{{clintonNum}}%</h1>
          </span>
        </div>
        <div class="row losers">
          <div class="col-md-6">
            <span ng-if="response">
              <img src="/images/Running-Donald_Trump.png" alt="" class="donald"><h3>{{trumpNum}}%</h3>
            </span>
          </div>
          <div class="col-md-6">
            <span ng-if="response">
              <img src="/images/gary-johnson-lab2-png.png" alt="" class="gary"><h3>{{johnsonNum}}%</h3>
            </span>
          </div>
        </div>
        <div class="col-md-9 donut">
        <!-- <canvas id="doughnut" class="chart chart-doughnut"
          chart-data="data" chart-labels="labels" chart-colors="colors">
        </canvas> -->
        </div>
      <!-- </div> -->
    </div>






  </body>
</html>
